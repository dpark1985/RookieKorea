<body ng-controller="detailCtrl">
    <header-custom></header-custom>

    <div class="wrapper" ng-include="curPage"></div>

    <footer-custom></footer-custom>
    <div id="isLogin">
        <%= user %>
    </div>
    <div id="toLoginModal" class="modal fade toLoginModal" tabindex="-1" role="dialog" aria-labelledby="toLoginModal">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">로그인</h4>
                </div>
                <div class="modal-body">
                    <h5 id="loginTitle">개인회원 로그인 후 이용할 수 있습니다.</h5>
                    <form id="loginForm" ng-submit="doLogin()">
                        <div class="form-group">
                            <label for="email">이메일</label>
                            <input type="email" class="form-control" id="email" placeholder="이메일">
                        </div>
                        <div class="form-group">
                            <label for="password">비밀번호</label>
                            <input type="password" class="form-control" id="password" placeholder="비밀번호">
                        </div>

                        <button type="submit" class="btn btn-default btn-lg btn-block">로그인</button>
                    </form>

                    <div class="links">
                        <span class="register"><a href="/register" target="_blank">회원가입</a></span>
                        <span class="resetpwd"><a href="/resetpwd" target="_blank">비밀번호 찾기</a></span>
                    </div>
                </div>
                <div class="modal-footer">

                </div>
            </div>
        </div>
    </div>


    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="../../libs/jquery/jquery-1.11.3.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="../../libs/bootstrap-3.3.5-dist/js/bootstrap.min.js"></script>

    <script type="text/javascript" src="//apis.daum.net/maps/maps3.js?apikey=4c26f6883fff6a7d1308c9ff28c36698"></script>

    <!-- Angular 1.4.4 -->
    <script src="../../libs/angular-1.4.4-dist/angular.min.js"></script>
    <script src="../../libs/angular-1.4.4-dist/angular-route.min.js"></script>

    <!-- Index Controller -->
    <script src="../../javascripts/controllers/detailsCtrl.js"></script>



</body>

</html>