    <% if(!everyauth.loggedIn) { %>
        <a href="/login">로그인</a>
        <a href="/register">회원가입</a>
    <% } else { %>

        <% if(userType == 'client') { %>
            <a href="/profile/user"><%= userID %></a>
        <% } else { %>

            <a href="/profile/coach"><%= userID %></a>
        <% } %>


        <a href="/logout">로그아웃</a>
    <% } %>

    
